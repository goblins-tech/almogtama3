{
  "name": "almogtama3",
  "version": "1.0.0",
  "scripts": {
    "build:browser": "ng build --aot --prod",
    "build:browser:dev": "ng build",
    "build:server": "ng run almogtama3:server:production --bundleDependencies all",
    "build:server:dev": "ng run almogtama3:server --bundleDependencies all",
    "build:server:webpack": "webpack --config webpack.server.config.js --progress --colors --mode=production",
    "build:server:webpack:dev": "webpack --config webpack.server.config.js --progress --colors --mode=development",
    "build:all": "npm run build:browser && npm run build:server && npm run build:server:webpack",
    "build:all:dev": "npm run build:browser:dev && npm run build:server:dev && npm run build:server:webpack:dev",
    "build": "npx cross-env NODE_ENV=production npm run build:all",
    "build:dev": "npx cross-env NODE_ENV=development npm run build:all:dev",
    "serve:ng": "ng serve -o",
    "serve": "node dist/server/express.js --start",
    "serve:inspect": "node --inspect dist/server/express.js --start",
    "serve:dev": "nodemon --inspect dist/server/express.js --start",
    "start": "npm run build && node task optimize && npm run serve",
    "start:dev": "npm run build:dev && npm run serve:inspect",
    "start:nodemon": "nodemon  src/ -e ts,html,scss --exec \"npm run start:dev\"",
    "firebase": "npm run firebase:build && npm run firebase:deploy",
    "firebase:build": "npm run build:server:webpack",
    "firebase:deploy": "firebase deploy --token 1//03o0ymiALXnyPCgYIARAAGAMSNwF-L9IrNSe7csFXkbFuGz1zgPc1SJzuohIvdneYBi-tlNJ6mARjWNLtwK9wpAzsCAx6r9hyYLY",
    "firebase:functions": "npm run firebase:build && npm run firebase:deploy -- --only functions",
    "firebase:serve": "firebase serve --port=4200 --token 1//03o0ymiALXnyPCgYIARAAGAMSNwF-L9IrNSe7csFXkbFuGz1zgPc1SJzuohIvdneYBi-tlNJ6mARjWNLtwK9wpAzsCAx6r9hyYLY",
    "firebase:token": "firebase login:ci",
    "firebase:start": "npm run build && npm run firebase:build && npm run firebase:serve",
    "firebase:start:dev": "npm run build:dev && npm run firebase:build && npm run firebase:serve",
    "dev:ssr": "ng run almogtama3:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run almogtama3:server:production",
    "prerender": "ng run almogtama3:prerender"
  },
  "engines": {
    "node": "10",
    "npm": "6.9.0"
  },
  "main": "dist/server/express.js",
  "private": false,
