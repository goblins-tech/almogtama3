  "scripts": {
    "build:browser": "ng build --aot --prod",
    "build:browser:dev": "ng build",
    "build:server": "ng run almogtama3:server:production --bundleDependencies all",
    "build:server:dev": "ng run almogtama3:server --bundleDependencies all",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build": "npm run build:browser && npm run build:server && npm run webpack:server",
    "build:dev": "npm run build:browser:dev && npm run build:server:dev && npm run webpack:server",
    "serve:ng": "ng serve -o",
    "serve": "node --inspect dist/server.js",
    "start": "npm run build && npm run serve",
    "start:dev": "npm run build:dev && npm run serve",
    "start:nodemon": "nodemon  src/ -e ts,html,scss --exec \"npm run start:dev\"",
    "firebase": "npm run firebase:build && npm run firebase:deploy",
    "firebase:build": "tsc -p firebase/tsconfig.json",
    "firebase:deploy": "firebase deploy --token 1//03hIcBxe_qHw9CgYIARAAGAMSNwF-L9IrhPsvqD_Ukl58YSK-Si0a5qBeOYoQtVWi78RCDBW6FDjHJy7e5B7XwPpw1qYXRGnctX8",
    "firebase:functions": "npm run firebase:build && npm run firebase:deploy -- --only functions",
    "firebase:serve": "firebase serve --port=4200 --token 1//03hIcBxe_qHw9CgYIARAAGAMSNwF-L9IrhPsvqD_Ukl58YSK-Si0a5qBeOYoQtVWi78RCDBW6FDjHJy7e5B7XwPpw1qYXRGnctX8",
    "firebase:token": "firebase login:ci",
    "firebase:start": "npm run build && npm run firebase:build && npm run firebase:serve",
    "firebase:start:dev": "npm run build:dev && npm run firebase:build && npm run firebase:serve"
  },
